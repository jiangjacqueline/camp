27. Remove Element
Link: https://leetcode.com/problems/remove-element/description/

class Solution {
    public int removeElement(int[] nums, int val) {
        if(nums == null || nums.length == 0){
            return 0;
        }

        int start = 0;
        for(int end = 0; end < nums.length; end++){
            if(nums[end] != val){
                nums[start] = nums[end];
                start++;
            }
        }
        return start;
    }
}
